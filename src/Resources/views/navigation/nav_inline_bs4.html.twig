<ul class="nav {{ level }}">
    {% for item in items %}
        <li class="nav-item {{ item.class }}">
            <a href="{{ item.href|default ? item.href : './' }}" title="{{ item.pageTitle|default ? item.pageTitle : item.title }}" class="nav-link {{ item.class }}" {{ item.accesskey is not same as('') ?  'accesskey="' ~ {{ item.accesskey }} ~ '"'}} {% if item.tabindex|default() %} tabindex="{{ item.tabindex }}"{% endif %} {% if item.nofollow|default() %} rel="nofollow"{% endif %}{{ item.target|raw }}{% if item.subitems|default %} aria-haspopup="true"{% endif %} itemprop="url"{% if item.isActive|default %} aria-current="page"{% endif %}>
                <span itemprop="name">
                    {{ item.link }}
                </span>
            </a>
            {{ item.subitems|default }}
        </li>
    {% endfor %}
</ul>