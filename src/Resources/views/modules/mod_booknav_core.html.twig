{% extends '@ContaoTwigTemplates/block/block_unsearchable_core.html.twig' %}

{% block content %}

<ul>
    {% if hasPrev|default %})
        <li class="previous"><a href="{{ prevHref ?: './' }}" title="{{ prevPageTitle|default }} ?>">{{ prevLink|default }}</a></li>
    {% else %}
        <li class="previous empty">&nbsp;</li>
    {% endif %}

    {% if hasUp|default %}
        <li class="up"><a href="{{ upHref ?: './' }}" title="{{ upPageTitle|default }} ?>">{{ upLink|default }}</a></li>
    {% else %}
        <li class="up empty">&nbsp;</li>
    {% endif %}

    {% if hasNext|default %}
        <li class="next"><a href="{{ nextHref ?: './' }}" title="{{ nextPageTitle|default }} ?>">{{ nextLink|default }}</a></li>
    {% else %}
        <li class="next empty">&nbsp;</li>
    {% endif %}
</ul>

{% endblock %}
